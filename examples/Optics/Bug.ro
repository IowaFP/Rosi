import Ro.Base , Ro.Show, Data.Tuple , Data.Nat , Data.List , Data.Tree , Data.Functor , Control.Monad

type r1 : (* -> *) -> *
type r1 = \ f. Pi { 'fmap := Functor f }

maybeFunctor : r1 Maybe
maybeFunctor = (#'fmap := /\ a b. \ f : a -> b. \ x : Maybe a. sorry)

type r2 : (* -> *) -> *
type r2 = \ f. Pi { 'fmap := Functor f , 'top := Unit }

maybeFunctor' : r2 Maybe
maybeFunctor' = (#'fmap := /\ a b. \ f. \ x. sorry , #'top := tt)


